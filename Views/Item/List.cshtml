@model Projeto.VewModel.ItemListViewModel
@{
    ViewData["Title"] = "Itens";
}
<h1>@Model.CategoriaAtual</h1>
<div class="container">
    <div class="row row-cols-sm-3 row-cols-1">
        @foreach (var item in Model.Itens)
        {
            <partial name="_Card" for="@item"></partial>
        }
    </div>
</div>